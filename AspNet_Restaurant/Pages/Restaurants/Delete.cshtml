@page "{restaurantId:int}"
@model AspNet_Restaurant.DeleteModel
@{
    ViewData["Title"] = "Delete";
}

<h2 class="label-danger">Are you sure you want to delete @Model.Restaurant.Name?</h2>
<form method="post">
    <button type="submit"
            class="btn btn-danger">
        Delete
    </button>
    <a class="btn btn-default"
       asp-page="List">
        Cancel
    </a>
</form>

